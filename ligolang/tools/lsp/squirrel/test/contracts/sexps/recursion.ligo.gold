(contract
  (fun
    rec
    sum
    (parameter (? n) int)
    (parameter (? acc) int)
    : int = (if (n < 1) acc (apply sum (n - 1) (acc + n))))
  (fun
    rec
    fibo
    (parameter (? n) int)
    (parameter (? n_1) int)
    (parameter (? n_0) int)
    : int = (if (n < 2) n_1 (apply fibo (n - 1) (n_1 + n_0) n_1))))
