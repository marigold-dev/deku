/* Simple test of binding multiple values */

let [x, y]: [int, int] = [1,2];

let main = (_ : unit): int => x + y;

let [x2, y2]: [int, int] = [3,3];

let main_paren = (_ : unit): int => x2 + y2;

let foobar : [int, int] = [23, 42];

let [foo, bar]: [int, int] = foobar;

let non_tuple_rhs = (_ : unit) : int => foo + bar;
