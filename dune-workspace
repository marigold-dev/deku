(lang dune 2.8)

(context default)

(context
 (default
  (name profiling)
  (instrument_with landmarks)
  (env
   (_
    (env-vars
     ("OCAML_LANDMARKS" "on"))))))

(context
 (default
  (name profiling-auto)
  (instrument_with landmarks)
  (env
   (_
    (env-vars
     ("OCAML_LANDMARKS" "auto"))))))
